<AnchorPane prefHeight="500.0" prefWidth="700.0" styleClass="root-pane" xmlns="http://javafx.com/javafx/10.0.2-internal" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controllers.EditBarProfilePageController">
    <GridPane gridLinesVisible="true" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
        <columnConstraints>
            <ColumnConstraints hgrow="NEVER" maxWidth="345.0" minWidth="10.0" prefWidth="220.0" />
            <ColumnConstraints hgrow="ALWAYS" maxWidth="Infinity" minWidth="10.0" prefWidth="480.0" />
        </columnConstraints>
        <rowConstraints>
            <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="ALWAYS" />
        </rowConstraints>
        <VBox prefHeight="200.0" prefWidth="100.0" spacing="15.0">
            <Button mnemonicParsing="false" onAction="#onGoToStartPageButtonClick" text="Spre pagina de start" textAlignment="CENTER">
                <VBox.margin>
                    <Insets left="10.0" top="10.0" />
                </VBox.margin>
            </Button>
            <ImageView fx:id="profilePhoto" fitHeight="150.0" fitWidth="200.0" pickOnBounds="true" preserveRatio="true" />
            <VBox alignment="TOP_CENTER" prefHeight="215.0" prefWidth="200.0" spacing="15.0" VBox.vgrow="ALWAYS">
                <VBox.margin>
                    <Insets />
                </VBox.margin>
                <Label text="Poți încărca o fotografie diferită" />
                <Button mnemonicParsing="false" onAction="#onChoosePhotoButtonClick" text="Alege fișier">
                    <VBox.margin>
                        <Insets top="-10.0" />
                    </VBox.margin>
                </Button>
                <TextField fx:id="nameField" alignment="CENTER" promptText="Nume Local">
                    <VBox.margin>
                        <Insets bottom="5.0" left="10.0" right="10.0" top="5.0" />
                    </VBox.margin>
                </TextField>
            </VBox>
            <HBox alignment="CENTER" prefHeight="5.0" prefWidth="200.0">
                <padding>
                    <Insets bottom="10.0" left="10.0" top="10.0" />
                </padding>
                <Button mnemonicParsing="false" onAction="#onSaveChangesButtonClick" text="Salvează modificări" textAlignment="CENTER" />
            </HBox>
            <padding>
                <Insets left="4.0" right="4.0" />
            </padding>
        </VBox>
        <GridPane alignment="CENTER" GridPane.columnIndex="1">
            <columnConstraints>
                <ColumnConstraints hgrow="SOMETIMES" maxWidth="500.0" minWidth="10.0" prefWidth="500.0" />
            </columnConstraints>
            <rowConstraints>
                <RowConstraints maxHeight="332.0" minHeight="10.0" prefHeight="234.0" vgrow="SOMETIMES" />
                <RowConstraints maxHeight="241.0" minHeight="10.0" prefHeight="166.0" vgrow="SOMETIMES" />

            </rowConstraints>
            <VBox prefHeight="200.0" prefWidth="100.0">
                <HBox alignment="CENTER" prefHeight="100.0">
                    <Label text="Adresa de email">
                        <HBox.margin>
                            <Insets left="10.0" />
                        </HBox.margin>
                    </Label>
                    <TextField fx:id="emailField" alignment="CENTER" disable="true" prefWidth="300.0" promptText="example@email.com" text="example@email.com">
                        <HBox.margin>
                            <Insets left="42.0" />
                        </HBox.margin>
                    </TextField>
                </HBox>
                <HBox alignment="CENTER" prefHeight="100.0" spacing="15.0">
                    <Label text="Tipul de utilizator">
                        <HBox.margin>
                            <Insets left="10.0" />
                        </HBox.margin>
                    </Label>
                    <TextField fx:id="userTypeField" alignment="CENTER" disable="true" prefWidth="300.0" promptText="Tip Utilizator" text="Tip Utilizator">
                        <HBox.margin>
                            <Insets left="20.0" />
                        </HBox.margin>
                    </TextField>
                </HBox>
                <HBox alignment="CENTER" prefHeight="100.0" spacing="15.0">
                    <Label text="Adresa localului">
                        <HBox.margin>
                            <Insets left="10.0" />
                        </HBox.margin>
                    </Label>
                    <TextField fx:id="addressField" prefWidth="300.0" promptText="Adresa Localului">
                        <HBox.margin>
                            <Insets left="30.0" />
                        </HBox.margin>
                    </TextField>
                </HBox>
                <HBox alignment="CENTER_LEFT" prefHeight="68.0" prefWidth="500.0">
                    <Label text="Orar disponibilitate:">
                        <HBox.margin>
                            <Insets left="20.0" />
                        </HBox.margin>
                    </Label>
                </HBox>
            </VBox>
            <GridPane fx:id="scheduleGridPane" alignment="CENTER" gridLinesVisible="true" prefHeight="177.0" prefWidth="480.0" GridPane.rowIndex="1">
                <columnConstraints>
                    <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
                    <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
                    <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
                    <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
                    <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
                    <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
                    <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
                    <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
                </columnConstraints>
                <rowConstraints>
                    <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                    <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                    <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                </rowConstraints>
                <GridPane.margin>
                    <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
                </GridPane.margin>
            </GridPane>
        </GridPane>
    </GridPane>
   <stylesheets>
      <URL value="@../../css/material-fx-v0_3.css" />
      <URL value="@../../css/stylesheet.css" />
   </stylesheets>
</AnchorPane>
