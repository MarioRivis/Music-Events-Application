        <HBox prefHeight="100.0" prefWidth="200.0">
            <VBox prefHeight="500.0" prefWidth="217.0" spacing="15.0">
                <ImageView fx:id="profilePhoto" fitHeight="150.0" fitWidth="200.0" pickOnBounds="true"
                           preserveRatio="true"/>
