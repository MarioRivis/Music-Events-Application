        </columnConstraints>
        <rowConstraints>
            <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="ALWAYS" />
        </rowConstraints>
        <GridPane alignment="CENTER" GridPane.columnIndex="1">
            <columnConstraints>
                <ColumnConstraints hgrow="SOMETIMES" maxWidth="500.0" minWidth="10.0" prefWidth="500.0" />
            </columnConstraints>
            <rowConstraints>
                <RowConstraints maxHeight="332.0" minHeight="10.0" prefHeight="234.0" vgrow="SOMETIMES" />
            <RowConstraints />

            </rowConstraints>
            <GridPane fx:id="scheduleGridPane" alignment="CENTER" gridLinesVisible="true" prefHeight="177.0" prefWidth="480.0" GridPane.rowIndex="1">
                <columnConstraints>
                    <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
                    <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
                    <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
                    <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
