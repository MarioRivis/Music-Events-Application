                    <HBox prefHeight="0.0" prefWidth="200.0"/>
                </VBox>
                <HBox alignment="CENTER" prefHeight="5.0" prefWidth="200.0">
                    <padding>
                        <Insets bottom="10.0" left="10.0" top="10.0"/>
                    </padding>
                    <VBox.margin>
                        <Insets top="-15.0"/>
                    </VBox.margin>
                    <Button defaultButton="true" mnemonicParsing="false" onAction="#onSaveChangesButtonClick"
                            styleClass="button-raised" text="Salvează modificări" textAlignment="CENTER"/>
                </HBox>
                <padding>
                    <Insets left="4.0" right="4.0" top="30.0"/>
                </padding>
            </VBox>
            <Separator orientation="VERTICAL" prefHeight="200.0">
                <HBox.margin>
                    <Insets bottom="-15.0" top="-15.0"/>
                </HBox.margin>
            </Separator>
        </HBox>
    </GridPane>
   <stylesheets>
       <URL value="@../../../css/material-fx-v0_3.css" />
       <URL value="@../../../css/stylesheet.css" />
   </stylesheets>
